{
  "name": "docker-cli-js",
  "version": "2.9.0",
  "description": "A node.js wrapper for the docker command line interface CLI",
  "main": "dist/index.js",
  "typings": "dist/index",
  "files": [
    "dist/"
  ],
  "scripts": {
    "lint": "eslint . --fix --ext .ts",
    "lint-fix": "eslint . --fix --ext .ts",
    "build": "npx del-cli dist/ && tsc",
    "test": "jest",
    "prepublish": "npm run lint && npm run build && rimraf \"dist/**/*.spec.*\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Quobject/docker-cli-js.git"
  },
  "keywords": [
    "docker",
    "docker cli"
  ],
  "author": "Matthias Ludwig <mludwig@quobject.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Quobject/docker-cli-js/issues"
  },
  "homepage": "https://github.com/Quobject/docker-cli-js",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/lodash.snakecase": "^4.1.6",
    "@types/moment-duration-format": "^2.2.3",
    "@types/node": "^14.14.25",
    "@types/parse-json": "^4.0.0",
    "@types/shelljs": "^0.8.9",
    "@types/tape": "^4.13.2",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "del-cli": "^3.0.1",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.3",
    "jest": "^26.6.3",
    "promise-delay": "^2.1.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "cli-table-2-json": "1.0.13",
    "dockermachine-cli-js": "3.0.5",
    "lodash.snakecase": "^4.1.1",
    "nodeify-ts": "1.0.6"
  }
}
